<h3>Add a New Blog Post</h3>
<%= form_with model: @blog do |entry| %>

    <%= entry.label :title %>
    <%= entry.text_field :title %>

    <br>
    <%= entry.label :content %>
    <%= entry.text_field :content %>

    <br>
    <%= entry.submit %>
    
    <% end %>

    <p>Back to <%= link_to 'All the Blogs!', blogs_path %></p>